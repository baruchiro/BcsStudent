{
  "name": "BscStudent",
  "version": "2.1.0",
  "private": true,
  "repository": "baruchiro/BcsStudent",
  "scripts": {
    "start": "next dev",
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "build": "cross-env INIT_CWD=$PWD next build && yarn postbuild",
    "postbuild": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir src --dir scripts",
    "rss": "node scripts/opml/opml.js",
    "prepare": "husky && node ./scripts/download-n8n-demo-scripts.js"
  },
  "dependencies": {
    "@calcom/embed-react": "^1.5.3",
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "@n8n/chat": "^0.27.1",
    "@next/bundle-analyzer": "14.2.33",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.22",
    "contentlayer": "0.3.4",
    "esbuild": "0.25.12",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "image-size": "1.2.1",
    "next": "14.2.33",
    "next-contentlayer": "0.3.4",
    "next-themes": "^0.2.1",
    "pliny": "0.4.1",
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-github-btn": "^1.4.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^1.0.3",
    "rehype-katex": "^6.0.3",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.6.3",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.3",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "tailwindcss": "^3.4.18",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/mdx": "^2.0.13",
    "@types/react": "^18.3.27",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.33",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^13.3.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/core": "1.13.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.39.1",
    "@opentelemetry/resources": "1.13.0",
    "@opentelemetry/sdk-trace-base": "1.13.0",
    "@opentelemetry/sdk-trace-node": "1.13.0",
    "@opentelemetry/semantic-conventions": "1.13.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.6.1",
  "engines": {
    "node": "20.x"
  }
